import java.util.*;
class sum{
    public static void main(String[]args){
        int n,rem,add=0;
        Scanner s=new Scanner(System.in);
        System.out.println("enter the number:");
        n=s.nextInt();
        while(n!=0){
            rem=n%10;
            add=add+rem;
            n=n/10;
        }
        System.out.println("the sum of n numbers is :"+add);
        
    }
}
